// 0.6 mm^2 power wires
// Serial power: the power lines do not accumulate
//0.5 SP chain per rod
Component {
  componentName "Power wires"
  service true
  scaleOnSensor 0
  // ONE wire
  Element {
    elementName Cu_SP_L2
    quantity 0.17
    unit g/m
  }
  // ONE wire
  Element {
    elementName PE_SP_L2
    quantity 0.69
    unit g/m
  }
  // ONE wire
  Element {
    elementName Al_SP_L2
    quantity 1.48
    unit g/m
  }
}

